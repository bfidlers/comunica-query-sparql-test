        {
            "name": "query_group_aggregates",
            "description": "Simple query to file with group aggregates",
            "query" : ["SELECT ?category (sum(?price) AS ?total)",
                        "WHERE {",
                        "   ?expense <http://mu.semte.ch/vocabularies/ext/category> ?category ;",
                        "            <http://mu.semte.ch/vocabularies/ext/amount> ?price .",
                        "}",
                        "GROUP BY ?category"
                        ],
            "source": "http://localhost:3000/turtle/expenses.ttl",
            "expected_output" : [
                {
                  "category": "\"Miscellaneous\"",
                  "total": "\"190\"^^http://www.w3.org/2001/XMLSchema#decimal"
                },
                {
                  "category": "\"Groceries\"",
                  "total": "\"140\"^^http://www.w3.org/2001/XMLSchema#decimal"
                }]
        },
        {
            "name": "query_nested",
            "description": "Simple nested query to file",
            "query" : ["SELECT ?category (sum(?price) AS ?total)",
                        "WHERE {",
                        "   ?expense <http://mu.semte.ch/vocabularies/ext/category> ?category ;",
                        "            <http://mu.semte.ch/vocabularies/ext/amount> ?price .",
                        "   {",
                        "       SELECT DISTINCT ?category",
                        "       WHERE {",
                        "           <http://mu.semte.ch/sessions/3a8c852e-888a-11ee-896a-0242ac14000a> <http://mu.semte.ch/vocabularies/session/account> ?account .",
                        "           ?account <http://mu.semte.ch/vocabularies/ext/expense> ?expense .",
                        "           ?expense <http://mu.semte.ch/vocabularies/ext/category> ?category .",
                        "       }",
                        "   }",
                        "}",
                        "GROUP BY ?category"
                        ],
            "source": "http://localhost:3000/turtle/expenses.ttl",
            "expected_output" : [
                {
                  "category": "\"Miscellaneous\"",
                  "total": "\"190\"^^http://www.w3.org/2001/XMLSchema#decimal"
                },
                {
                  "category": "\"Groceries\"",
                  "total": "\"140\"^^http://www.w3.org/2001/XMLSchema#decimal"
                }]
        },
        {
            "name": "query_bind",
            "description": "Simple query to file with binds",
            "query" : ["PREFIX ext: <http://mu.semte.ch/vocabularies/ext/>",
                        "SELECT ?name ?born ?died ?n1 ?n2 ?n3 ?ageInDays ?ageInDays2 ?ageInYears WHERE {",
                        "  ?id ext:name ?name;",
                        "      ext:birthdate ?born;",
                        "      ext:deathdate ?died;",
                        "      ext:n1 ?n1;",
                        "      ext:n2 ?n2.",
                        "  BIND(?n1 - ?n2 AS ?n3).",
                        "  BIND(?died - ?born AS ?ageInDays).",
                        "  BIND(day(?died) - day(?born) AS ?ageInDays2).",
                        "  BIND(year(?died) - year(?born) AS ?ageInYears).",
                        "}"
                        ],
            "source": "http://localhost:3000/turtle/person.ttl",
            "expected_output" : [
                {
                  "n1": "\"10\"^^http://www.w3.org/2001/XMLSchema#integer",
                  "n2": "\"5\"^^http://www.w3.org/2001/XMLSchema#integer",
                  "died": "\"2050-12-06\"^^http://www.w3.org/2001/XMLSchema#date",
                  "n3": "\"5\"^^http://www.w3.org/2001/XMLSchema#integer",
                  "born": "\"2022-12-06\"^^http://www.w3.org/2001/XMLSchema#date",
                  "name": "\"Jimmy\"",
                  "ageInYears": "\"28\"^^http://www.w3.org/2001/XMLSchema#integer",
                  "ageInDays2": "\"0\"^^http://www.w3.org/2001/XMLSchema#integer",
                  "ageInDays": "\"P10227D\"^^http://www.w3.org/2001/XMLSchema#dayTimeDuration"
                },
                {
                  "n1": "\"5\"^^http://www.w3.org/2001/XMLSchema#integer",
                  "n2": "\"10\"^^http://www.w3.org/2001/XMLSchema#integer",
                  "died": "\"2022-12-07\"^^http://www.w3.org/2001/XMLSchema#date",
                  "n3": "\"-5\"^^http://www.w3.org/2001/XMLSchema#integer",
                  "born": "\"2022-12-06\"^^http://www.w3.org/2001/XMLSchema#date",
                  "name": "\"Willy\"",
